{
    "manifest_version": 2,
    "name": "ReElon",
    "version": "1.0",
    "description": "Replaces a specific image URL with another on a specific site.",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "activeTab",
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "reelon@email.com",
            "strict_min_version": "42.0"
        }
    }
}